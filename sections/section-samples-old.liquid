<script>
  // Preload inventory data
  const inventoryData = {
    {% for product in collections['Samplepage'].products %}
      {% for variant in product.variants %}
        "{{ variant.id }}": {
          available: {{ variant.available | json }}
        },
      {% endfor %}
    {% endfor %}
  };
  
  var variantPrices = {
    {% for product in collections['Samplepage'].products %}
      {% for variant in product.variants %}
        "{{ variant.id }}": {{ variant.price | divided_by: 100 }},
      {% endfor %}
    {% endfor %}
  };

  function showPopup(productName, productImage, details) {
    const popupOverlay = document.getElementById('popup-overlay');
    const popupInfo = document.getElementById('popup-info');

    // Clear previous content
    popupInfo.innerHTML = '';

    // Add product name
    const nameElement = document.createElement('h2');
    nameElement.className = 'product-name';
    nameElement.textContent = productName;
    popupInfo.appendChild(nameElement);

    // Add product image
    const imageElement = document.createElement('img');
    imageElement.src = productImage;
    imageElement.alt = productName;
    imageElement.className = 'product-image';
    popupInfo.appendChild(imageElement);

    // Create the detail grid
    const detailGrid = document.createElement('div');
    detailGrid.className = 'detail-grid';

    // Add blocks for top-left, bottom-left, top-right, bottom-right
    const blocks = [
      {
        position: 'top-left',
        icon: details.topLeft.icon,
        alt: details.topLeft.alt,
        largeText: details.topLeft.largeText,
        smallText: details.topLeft.smallText,
        textColor: details.topLeft.textColor,
      },
      {
        position: 'bottom-left',
        icon: details.bottomLeft.icon,
        alt: details.bottomLeft.alt,
        largeText: details.bottomLeft.largeText,
        smallText: details.bottomLeft.smallText,
        textColor: details.bottomLeft.textColor,
      },
      {
        position: 'top-right',
        icon: details.topRight.icon,
        alt: details.topRight.alt,
        largeText: details.topRight.largeText,
        smallText: details.topRight.smallText,
        textColor: details.topRight.textColor,
      },
      {
        position: 'bottom-right',
        icon: details.bottomRight.icon,
        alt: details.bottomRight.alt,
        largeText: details.bottomRight.largeText,
        smallText: details.bottomRight.smallText,
        textColor: details.bottomRight.textColor,
      },
    ];

    blocks.forEach((block) => {
      const blockElement = document.createElement('div');
              blockElement.className = `detail-block ${block.position}`;

              // Add icon
        const iconElement = document.createElement('img');
        iconElement.src = block.icon;
        iconElement.alt = block.alt;
        iconElement.className = 'detail-icon';
        blockElement.appendChild(iconElement);

        // Add text container
        const textContainer = document.createElement('div');
        textContainer.className = 'detail-text';

        // Add large text
        const largeTextElement = document.createElement('p');
        largeTextElement.className = 'large-text';
        largeTextElement.textContent = block.largeText;
        largeTextElement.style.color = block.textColor;
        textContainer.appendChild(largeTextElement);

        // Add small text
        const smallTextElement = document.createElement('p');
        smallTextElement.className = 'small-text';
        smallTextElement.textContent = block.smallText;
        smallTextElement.style.color = block.textColor;
        textContainer.appendChild(smallTextElement);

      blockElement.appendChild(textContainer);
      detailGrid.appendChild(blockElement);
    });

    popupInfo.appendChild(detailGrid);

          // Show the popup
      popupOverlay.style.display = 'flex';
    }

    function closePopup() {
      const popupOverlay = document.getElementById('popup-overlay');
      popupOverlay.style.display = 'none';
    }

    function updateTotalPrice() {
      let total = 0;   
      selectedSamples.forEach(sample => {
        let price = variantPrices[sample.id] || 0;
        total += price * sample.quantity;
      });  
      document.getElementById("total-price-display").textContent = `$${total.toFixed(2)}`;
    }
      
    function updateAddToCartButton() {
      let total = 0;   
      selectedSamples.forEach(sample => {
        let price = variantPrices[sample.id] || 0;
        total += price * sample.quantity;
      }); 
      const mobileButton = document.querySelector(".choice-progress.mobile #num-samples-selected");
      if (mobileButton) {
        mobileButton.textContent = `${selectedSamples.length}/5 {% if canonical_url contains '/zh/' %}已選{% else %}SELECTED{% endif %} - $${total.toFixed(2)}`;
        // Trigger animation after everything is done 
        triggerBounceAnimation();
      }
    }

    // Function to animate the padding change with a bounce effect
    function triggerBounceAnimation() {
      const element = document.querySelector('.choice-progress.mobile');
      if (!element) return; 
    
      element.style.padding = "3vw 0 2vw 0";
      element.style.transition = "padding 0.4s cubic-bezier(0.34, 1.56, 0.64, 1)"; // Bounce effect
      element.style.padding = "3vw 0 7vw 0"; // Expand
  
      setTimeout(() => {
        element.style.padding = "3vw 0 2vw 0"; // Return to original size
      }, 300);
    }

    function changeButtonStyle(selectedButton) {
      let buttons = document.querySelectorAll('.button-row button');
      buttons.forEach(button => button.classList.remove('active'));
      selectedButton.classList.add('active');

      // Get the filter category based on the clicked button's id
      let filterCategory = selectedButton.id;
      
      // Call the function to filter samples based on the selected category
      filterSamples(filterCategory);
    }

    function filterSamples(category) {
      const samples = document.querySelectorAll('.samples-item');
  
      // Create a Set of allowed categories
      let allowedClasses = new Set();
      if (category === 'all-button') {
        allowedClasses = new Set(['kibble', 'treats', 'dental-treats', 'small-dog-friendly']);
      } else if (category === 'kibble-button') {
        allowedClasses = new Set(['kibble']);
      } else if (category === 'treats-button') {
        allowedClasses = new Set(['treats', 'dental-treats']);
      } else if (category === 'for-small-button') {
        allowedClasses = new Set(['small-dog-friendly']);
      }
  
      // Use classList.contains() only once per sample, reducing reflow/repaint
      samples.forEach(sample => {
        const overlay = sample.querySelector('.overlay-item');
        const isMatch = [...allowedClasses].some(cls => sample.classList.contains(cls));
        const variantId = sample.querySelector('.quantity-selector').getAttribute('data-variant-id');
        const inventory = inventoryData[variantId];
        const isAvailable = inventory.available;
        

        if (!isMatch || !isAvailable) {
          overlay.style.display = 'block';
    
        } else {
          overlay.style.display = 'none';
        }
      });
    }

    function toggleGiftBlock() {
      const content = document.querySelector('.gift-content');
      const icon = document.querySelector('.dropdown-icon');
      content.classList.toggle('open');
      icon.classList.toggle('rotate');
    }

    function toggleSampleBlock() {
      const contentTwo = document.querySelector('.sample-content');
      const iconTwo = document.querySelector('.dropdown-icon-2');
      contentTwo.classList.toggle('open');
      iconTwo.classList.toggle('rotate');
    }

    let selectedItems = [];
    let selectedSamples = [];

    // Toggle selection and manage selected variant IDs for bowls
    function toggleAddToCart(button) {
      const variantId = button.getAttribute('data-variant-id');
      const allButtons = document.querySelectorAll(`button[data-variant-id="${variantId}"]`);
  
      // Toggle selected state for all buttons with the same variantId
      allButtons.forEach(btn => {
        if (btn === button) {
          btn.classList.toggle('selected');
          btn.textContent = btn.classList.contains('selected') ? 'Selected' : '+';
        } else {
          btn.classList.add('selected');
          btn.textContent = 'Selected';
        }
      });
  
      if (button.classList.contains('selected')) {
        // Deselect the other bowl if one is chosen
        if (variantId === '45058917105979' || variantId === '45058917073211') {
          const otherVariantId = variantId === '45058917105979' ? '45058917073211' : '45058917105979';
          const otherButtons = document.querySelectorAll(`button[data-variant-id="${otherVariantId}"]`);
  
          otherButtons.forEach(otherBtn => {
            otherBtn.classList.remove('selected');
            otherBtn.textContent = '+';
          });
  
          // Remove the other bowl from selectedItems
          selectedItems = selectedItems.filter(item => item.id !== parseInt(otherVariantId));
        }
  
        // Add the selected item
        if (!selectedItems.some(item => item.id === parseInt(variantId))) {
          selectedItems.push({ id: parseInt(variantId), quantity: 1 });
        }
      } else {
        // Remove deselected item
        selectedItems = selectedItems.filter(item => item.id !== parseInt(variantId));
      }
  
      // Check if Canvas Bag and one Bowl are selected
      const canvasBagSelected = selectedItems.some(item => item.id === 49678717911355);
      const bowlSelected = selectedItems.some(item => item.id === 45058917105979 || item.id === 45058917073211);
  
      if (canvasBagSelected && bowlSelected) {
        setTimeout(() => {
          toggleGiftBlock();
          toggleSampleBlock();
        }, 500);
      }
    }

    function addAllToCart() {
      // Check if selectedSamples is empty
      if (selectedSamples.length === 0) {
        {% if canonical_url contains '/zh/' %}
        alert("請選取至少一款狗糧試食包");
        {% else %}
        alert("Please select at least one sample");  
        {% endif %}
        return;
      }
      
      // Combine selectedItems and selectedSamples arrays
      const combinedItems = selectedItems.concat(selectedSamples);
      
      // First add items to cart
      fetch('/cart/add.js', {
        method: 'POST',
        headers: { 
          'Content-Type': 'application/json',
          'Accept': 'application/json'
        },
        credentials: 'same-origin',
        body: JSON.stringify({ items: combinedItems })
      })
      .then(response => {
        if (!response.ok) {
          return response.json().then(err => { throw new Error(err.description); });
        }
        return response.json();
      })
      .then(() => {
        updateAddToCartButton();
        
        // Create and submit a hidden form to apply discount
        const discountForm = document.createElement('form');
        discountForm.style.display = 'none';
        discountForm.method = 'GET';
        discountForm.action = '/discount/SAMPLE-GIFTS';
        
        // Add a hidden iframe to prevent page navigation
        const iframe = document.createElement('iframe');
        iframe.name = 'discount-frame';
        iframe.style.display = 'none';
        document.body.appendChild(iframe);
        
        // Target the form submission to the iframe
        discountForm.target = 'discount-frame';
        document.body.appendChild(discountForm);
        
        // Submit the form
        discountForm.submit();
        
        // Reset button state and show success message after a brief delay
        setTimeout(() => {              
          // Clean up DOM
          document.body.removeChild(discountForm);
          document.body.removeChild(iframe);
        }, 1000);
      })
      .catch(error => {
        console.error('Error in cart operations:', error);
        {% if canonical_url contains '/zh/' %}
        alert("處理您的請求時出現問題, 請再試一次");
        {% else %}
        alert('There was an issue processing your request. Please try again.');
        {% endif %}
      });
    }

    document.querySelectorAll('.quantity-selector').forEach(selector => {
      const decrement = selector.querySelector('.decrement');
      const increment = selector.querySelector('.increment');
      const quantityDisplay = selector.querySelector('.quantity');
      const variantId = selector.getAttribute('data-variant-id');

      // Helper function to update selectedSamples
      const updateSelectedSamples = (action) => {
        if (action === 'add') {
          selectedSamples.push({ id: parseInt(variantId), quantity: 1 });
        } else if (action === 'remove') {
          // Remove one occurrence of the variantId from selectedSamples
          const index = selectedSamples.findIndex(item => item.id === parseInt(variantId));
          if (index > -1) {
            selectedSamples.splice(index, 1);
          }
        }
        updateAddToCartButton(); // Update mobile button dynamically
        updateTotalPrice();
      };

      const updateBackground = (count) => {
        if (count > 0) {
          selector.style.backgroundColor = '#fff'; 
          selector.style.color = '#193c6b'; 
          decrement.style.color = '#193c6b'; 
          increment.style.color = '#193c6b';
        } else {
          selector.style.backgroundColor = '#193c6b';
          selector.style.color = '#fff'; 
          decrement.style.color = '#fff'; 
          increment.style.color = '#fff';
        }
      };

      increment.addEventListener('click', () => {
        let count = parseInt(quantityDisplay.textContent, 10);
        if (selectedSamples.length < 5) {
          count++;
          quantityDisplay.textContent = count;
          updateBackground(count);
          updateSelectedSamples('add');
          addProductImage(variantId);
        } else {
          {% if canonical_url contains '/zh/' %}
          alert("你最多可選取5款產品");
          {% else %}
          alert("You can select a maximum of 5 items.");
          {% endif %} 
        }
      });

      decrement.addEventListener('click', () => {
        let count = parseInt(quantityDisplay.textContent, 10);
        if (count > 0) {
          count--;
          quantityDisplay.textContent = count;
          updateBackground(count);
          updateSelectedSamples('remove');
          removeProductImage(variantId);
        }
      });
    });

    // Track available indices for product placement
    let availableIndices = [1, 2, 3, 4, 5];
    
    // Map variant IDs to their respective image URLs
    const variantImages = {
      "47517822484795": "https://cdn.shopify.com/s/files/1/0430/5833/1805/files/BB_Duck_FOP_Angled_a71c00c1-9a7f-4a4d-98f6-6672ede83454_600x.png?v=1742008000",
      "47517965451579": "https://cdn.shopify.com/s/files/1/0430/5833/1805/files/BB_Puppy_FOP_Angled_600x.png?v=1742008000",
      "47517935337787": "https://cdn.shopify.com/s/files/1/0430/5833/1805/files/BB_Salmon_FOP_Angled_600x.png?v=1742008001",
      "47517956112699": "https://cdn.shopify.com/s/files/1/0430/5833/1805/files/BB_Turkey_FOP_Angled_600x.png?v=1742008001",
      "47517828677947": "https://cdn.shopify.com/s/files/1/0430/5833/1805/files/BB_Lamb_FOP_Angled_f8b71bd8-4b12-447b-8693-d31f0c12ccfd_600x.png?v=1742008000",
      "48424319582523": "https://cdn.shopify.com/s/files/1/0430/5833/1805/files/Chicken_Honey_SOP_Shadow_600x.png?v=1742007998",
      "48424321679675": "https://cdn.shopify.com/s/files/1/0430/5833/1805/files/Beef_Blueberry_SOP_Shadow_600x.png?v=1742007998",
      "42386410209437": "https://cdn.shopify.com/s/files/1/0430/5833/1805/files/Dental_Sticks_SOP_Shadow_03_600x.png?v=1742007998"
    };
    
    // Function to add a product image to the first available placeholder
    function addProductImage(variantId) {
      const index = availableIndices.shift(); 
      const placeholder = document.querySelector(`.product-placeholder:nth-child(${index + 1})`);
      placeholder.style.backgroundImage = `url('${variantImages[variantId]}')`;
    }
    
    // Function to remove a product image and free up its index
    function removeProductImage(variantId) {
      const placeholders = document.querySelectorAll('.product-placeholder');
      
      // Find the last placeholder that contains the product image
      for (let i = placeholders.length - 1; i >= 0; i--) {
        if (placeholders[i].style.backgroundImage.includes(variantImages[variantId])) {
          placeholders[i].style.backgroundImage = ''; 
          availableIndices.push(i + 1); 
          availableIndices.sort(); 
          break; 
        }
      }
    }

    document.addEventListener('DOMContentLoaded', function () {
      document.querySelectorAll('.button-row button')[0].classList.add('active');
      toggleGiftBlock();
      filterSamples('all-button');
      updateAddToCartButton();
    });
</script>

